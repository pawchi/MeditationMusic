<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.chilon.meditationmusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PlayMusicOne"
            android:label="@string/title_activity_play_music"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.chilon.meditationmusic.MainActivity" />
        </activity>

        <activity
            android:name=".PopSetupWindow"
            android:theme="@style/AppTheme.CustomTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.chilon.meditationmusic.PlayMusicOne" />

    </activity>

        <activity android:name=".ActivityMainSetup" />
        <activity android:name=".Activity_rate_us"></activity>
    </application>

</manifest>